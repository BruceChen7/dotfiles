{
  "context_servers": {
    "repomix": {
      "source": "custom",
      "enabled": true,
      "command": "npx",
      "args": ["-y", "repomix", "--mcp"],
      "env": {}
    },
    "mindpilot": {
      "source": "custom",
      "enabled": true,
      "command": "npx",
      "args": ["@mindpilot/mcp@latest"],
      "env": {}
    }
  },
  "agent": {
    "default_profile": "write",
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "openai",
      "model": "qwen3-next-80b-a3b-instruct"
    },
    "thread_summary_model": {
      "provider": "openai",
      "model": "kimi-k2"
    },
    "expand_edit_card": true
  },
  "vim_mode": true,
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "ui_font_size": 18,
  "buffer_font_size": 18,
  "buffer_font_family": "Iosevka Nerd Font",
  "theme": {
    "mode": "system",
    "light": "One Dark",
    "dark": "One Dark"
  },
  "terminal": {
    "dock": "bottom",
    "font_family": "Iosevka Term"
  },
  "language_models": {
    "openai": {
      // "api_url": "https://ai.nahcrof.com/v2",
